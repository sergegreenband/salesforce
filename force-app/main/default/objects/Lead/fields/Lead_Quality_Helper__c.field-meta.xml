<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lead_Quality_Helper__c</fullName>
    <description>Evaluate these 5 fields: Email, Phone, Company, Title, and Industry</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Email), 0, 1) + 
 IF(ISBLANK(Phone), 0, 1) +  
 IF(ISBLANK(Company), 0, 1) +
 IF(ISBLANK(Title), 0, 1) + 
 IF(ISPICKVAL(Industry, &quot;&quot;), 0, 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Lead Quality Helper</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
