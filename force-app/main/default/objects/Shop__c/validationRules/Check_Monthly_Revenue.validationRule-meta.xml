<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Monthly_Revenue</fullName>
    <active>true</active>
    <errorConditionFormula>OR (Monthly_Revenue__c &lt;=0,  ISBLANK(Monthly_Revenue__c) )</errorConditionFormula>
    <errorMessage>Not enough monthly revenue!</errorMessage>
</ValidationRule>
